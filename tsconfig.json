{
  "include": ["src/**/*"],
  "files": ["./src/index.tsx"],
  "compilerOptions": {
    "moduleResolution": "node",
    "jsx": "react",
    "strict": true,
    "sourceMap": true,
    "target": "ES6",
    "types": ["webpack-env"],
    "baseUrl": "./src",
    "paths": {
      // "src/*": ["./src/*"]
    },
    "allowSyntheticDefaultImports": true,
    "lib": [
      "ES6",
      "DOM",
      "DOM.Iterable"
    ]
  }
}